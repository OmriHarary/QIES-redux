{
   // Use IntelliSense to find out which attributes exist for C# debugging
   // Use hover for the description of the existing attributes
   // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
   "version": "0.2.0",
   "configurations": [
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Web",
            "program": "${workspaceFolder}/QIES/src/QIES.Web/bin/Debug/net5.0/QIES.Web.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "COMPlus_ZapDisable": "1",
                "COMPlus_ReadyToRun": "0"
            },
            "justMyCode": false,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": true
            },
            "suppressJITOptimizations": true
        },
        {
            "name": ".NET Core Launch Cli (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build CLI",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/QIES.Cli/src/bin/Debug/net5.0/QIES.Cli.dll",
            "args": ["../test/resources/valid-services-list.txt", "../trans-summary"],
            "cwd": "${workspaceFolder}/QIES.Cli/src",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "justMyCode": false,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": true
            },
            "suppressJITOptimizations": true,
            "env": {
                "COMPlus_ZapDisable": "1",
                "COMPlus_ReadyToRun": "0"
            }
        },
        {
            "name": ".NET Core Launch Backoffice (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build backoffice",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/QIES.Backoffice/src/bin/Debug/net5.0/QIES.Backoffice.dll",
            "args": [
                "../test/resources/merged_transaction_summery_file.txt",
                "../test/resources/old_central_services_file.txt",
                "../new-central-services",
                "../new-valid-services"
            ],
            "cwd": "${workspaceFolder}/QIES.Backoffice/src",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "justMyCode": false,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": true
            },
            "suppressJITOptimizations": true,
            "env": {
                "COMPlus_ZapDisable": "1",
                "COMPlus_ReadyToRun": "0"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
